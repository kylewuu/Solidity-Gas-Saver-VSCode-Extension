{
  "name": "soliditygassaver",
  "displayName": "SolidityGasSaver",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "soliditygassaver.packvariablesfirstfit",
        "title": "Pack Variable First Fit"
      },
      {
        "command": "soliditygassaver.packvariablesbestfit",
        "title": "Pack Variable Best Fit"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.18.14",
    "@types/vscode": "^1.76.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.35.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4"
  },
  "languages": [
    {
      "id": "solidity",
      "extensions": [
        ".sol"
      ],
      "aliases": [
        "Solidity",
        "solidity"
      ]
    }
  ],
  "dependencies": {
    "solc": "^0.8.19",
    "solc-typed-ast": "^13.0.0"
  }
}
